{
  "name": "live-trains-finland",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/use-sync-external-store": "^0.0.3",
    "autosuggest-highlight": "^3.3.4",
    "buffer": "^6.0.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "deck.gl": "^8.9.3",
    "graphql": "^16.6.0",
    "hsl-map-style": "github:HSLdevcom/hsl-map-style#master",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^3.0.0-pre.4",
    "mdi-material-ui": "^7.6.0",
    "mqtt": "^4.3.7",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-map-gl": "^7.0.21",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-svg": "^16.1.4",
    "typescript": "^4.9.5",
    "url": "^0.11.0",
    "use-local-storage-state": "^18.1.2",
    "use-sync-external-store": "^1.2.0",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts --max_old_space_size=6144 start",
    "build": "npm run extract-tiles && react-scripts --max_old_space_size=6144 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --max-warnings=0 ./src",
    "lint:fix": "eslint --fix ./src",
    "type-check": "tsc --noEmit",
    "extract-tiles-railway-tracks": "make-dir public/tiles && mbtiles-extractor --input=./data/railway_tracks.mbtiles --outputType=local --localOutDir=./public/tiles --tileDir=railway_tracks",
    "extract-tiles-railway-platforms": "make-dir public/tiles && mbtiles-extractor --input=./data/railway_platforms.mbtiles --outputType=local --localOutDir=./public/tiles --tileDir=railway_platforms",
    "extract-tiles": "npm run extract-tiles-railway-tracks && npm run extract-tiles-railway-platforms",
    "digitransit-codegen": "graphql-codegen --config digitransit-codegen.yml",
    "digitraffic-codegen": "graphql-codegen --config digitraffic-codegen.yml",
    "gql-codegen": "npm run digitransit-codegen && npm run digitraffic-codegen",
    "prestart": "npm run gql-codegen",
    "prepare": "is-ci || husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230321.0",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "make-dir-cli": "^3.0.0",
    "mbtiles-extractor": "^0.1.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/graphql/generated/*"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --max-warnings=0 ./src"
  }
}
