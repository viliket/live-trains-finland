{
  "name": "live-trains-finland",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/lab": "^5.0.0-alpha.98",
    "@mui/material": "^5.10.4",
    "@react-spring/web": "^9.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/jest": "^29.0.0",
    "@types/leaflet": "^1.7.11",
    "@types/leaflet.vectorgrid": "^1.3.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autosuggest-highlight": "^3.3.4",
    "buffer": "^6.0.3",
    "date-fns": "^2.29.2",
    "graphql": "^16.6.0",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "leaflet": "^1.8.0",
    "leaflet.vectorgrid": "^1.3.0",
    "lodash": "^4.17.21",
    "mdi-material-ui": "^7.5.0",
    "mqtt": "^4.3.7",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-leaflet": "^4.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-svg": "^15.1.8",
    "typescript": "^4.8.3",
    "url": "^0.11.0",
    "use-local-storage-state": "^18.1.1",
    "web-vitals": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run extract-tiles && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract-tiles-railway-tracks": "mbtiles-extractor --input=./data/railway_tracks.mbtiles --outputType=local --localOutDir=./public/tiles --tileDir=railway_tracks",
    "extract-tiles-railway-platforms": "mbtiles-extractor --input=./data/railway_platforms.mbtiles --outputType=local --localOutDir=./public/tiles --tileDir=railway_platforms",
    "extract-tiles": "npm run extract-tiles-railway-tracks && npm run extract-tiles-railway-platforms",
    "digitransit-codegen": "graphql-codegen --config digitransit-codegen.yml",
    "digitraffic-codegen": "graphql-codegen --config digitraffic-codegen.yml",
    "gql-codegen": "npm run digitransit-codegen && npm run digitraffic-codegen",
    "prestart": "npm run gql-codegen",
    "prepare": "is-ci || husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "mbtiles-extractor": "^0.1.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!react-leaflet|@react-leaflet/core)"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --max-warnings=0 ./src"
  }
}
